#include <stdio.h>
#include <stdlib.h>


int main()
{
  int grade;
 
  printf("Simple Grade Program\n");
  
  printf("Enter your grade: ");
  scanf("%d", &grade);
 
  if (grade >= 75) {
    // NESTED IF
    if (grade > 100)
     printf("INVALID GRADE.\n");
    else
     printf("PASSED.\n");
    
   } else {
      if (grade < 0)
        printf("INVALID GRADE.\n");
       else
         printf("FAILED.\n");
   }
  
//Codeblocks - codes inside curly braces{}
//print "PASSED" if grade is atleast 75
//otherwise print "FAILED"
  
  
  return 0;
  
}
